[run]
branch = True
source = src
omit =
    */conftest.py
    */test_*_fuzz.py
    */assert_valid_name.py
    */cached_property.py
    */character_classes.py
    */is_iterable.py

[report]
exclude_lines =
    pragma: no cover
    except ImportError:
    \# Python <
    raise NotImplementedError
    raise TypeError\(f?"Unexpected
    assert False,
    \s+next\($
    if MYPY:
    if TYPE_CHECKING:
    ^\s+\.\.\.$
    ^\s+pass$
ignore_errors = True
